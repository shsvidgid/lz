-- Ashle M1
AddContent("TextButton", "变成无敌爱吃菜", [[
local args = {"M1"}
local ashleRemote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Ashle")
while true do
    task.wait(0.05)
    ashleRemote:FireServer(unpack(args))
end
]])

