local Tab7 = Window:MakeTab({Name = "SBDEé‡ç½®ç‰ˆ", Icon = 4483345998, PremiumOnly = false})
local Section4 = Tab7:AddSection({Name = "æ”¾ç½®"})
local towerNames = {"Coil", "Default", "Cult", "Bull", "Warp", "Berserk", "Snow"}
local rs = game:GetService("ReplicatedStorage")

local function getArgs(tower)
    local part = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    return part and {rs.Towers[tower], CFrame.new(part.Position)} or {}
end

for _, tower in ipairs(towerNames) do
    Section4:AddButton({Name = "ç”Ÿæˆ".. ğŸ¤“ğŸ¤“ğŸ¤“ğŸ¤‘ğŸ¤‘ğŸ¤‘ğŸ˜ğŸ˜ğŸ˜,
        Callback = function()
            local args = getArgs(tower)
            if #args > 0 then
                rs.Functions.SpawnTower:InvokeServer(unpack(args))
            end
        end
    })
end
OrionLib:Init()
