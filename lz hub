local Tab4 = Window:MakeTab({Name = "SBDEå›æº¯ç‰ˆæœ¬", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local Section4 = Tab4:AddSection({Name = "æ”¾ç½®"})
local backtrackTowerNames = {"Coil", "Default", "Time", "Cult", "Bull", "Replica", "Killstreak", "Fort", "Warp", "Archer", "Gods Hand", "Berserk", "Slicer", "Snow", "Orb"}
local function getSpawnArgs(towerName)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        local playerPosition = humanoidRootPart.Position
        return {[1] = towerName, [2] = CFrame.new(playerPosition.X, playerPosition.Y, playerPosition.Z, 1, 0, 0, 0, 1, 0, 0, 0, 1)}
    end
    return {}
end
for _, towerName in ipairs(backtrackTowerNames) do
    Section4:AddButton({Name = "ç”Ÿæˆ".. towerName, Callback = function()
        local args = getSpawnArgs(towerName)
        if next(args) ~= nil then
            local service = game:GetService("ReplicatedStorage")
            local func = service:WaitForChild("Functions"):WaitForChild("SpawnTower")
            func:InvokeServer(unpack(args))
        end
    end})
end
local Tab5 = Window:MakeTab({Name = "è¿ç»­å‡»è´¥æ¨¡å¼", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local Section5 = Tab5:AddSection({Name = "åˆ·å‡»è´¥"})
local loopCountInput = 1
local intervalInput = 1
Section5:AddTextbox({Name = "å¾ªç¯æ•°é‡", Default = "1", TextDisappear = true, Callback = function(Value) loopCountInput = tonumber(Value) or 10 end})
Section5:AddTextbox({Name = "é—´éš”æ—¶é—´ï¼ˆç§’ï¼‰", Default = "0.1", TextDisappear = true, Callback = function(Value) intervalInput = tonumber(Value) or 1 end})
local function getDefeat()
    local loopCount = 0
    while loopCount < loopCountInput do
        game:GetService("ReplicatedStorage"):WaitForChild("ASD"):FireServer()
        loopCount = loopCount + 1
        wait(intervalInput)
    end
end
Tab5:AddButton({Name = "å¾—åˆ°å‡»è´¥", Callback = getDefeat})

-- æ–°å¢çš„æ— é™å¾ªç¯è·å–å‡»è´¥ç›¸å…³ä»£ç 
local isRunningInfiniteDefeat = false
local infiniteDefeatInterval = 0.1 -- é»˜è®¤çš„è§¦å‘é—´éš”æ—¶é—´ï¼Œå•ä½ç§’
local function updateInfiniteDefeatInterval(Value)
    infiniteDefeatInterval = tonumber(Value) or 0.1
end
Section5:AddTextbox({Name = "æ— é™å‡»è´¥è§¦å‘é—´éš”ï¼ˆç§’ï¼‰", Default = "0.1", TextDisappear = true, Callback = updateInfiniteDefeatInterval})
Tab5:AddToggle({
    Name = "æ— é™å¾ªç¯è·å¾—å‡»è´¥",
    Default = false,
    Callback = function(v)
        isRunningInfiniteDefeat = v
        if isRunningInfiniteDefeat then
            while isRunningInfiniteDefeat do
                game:GetService("ReplicatedStorage"):WaitForChild("ASD"):FireServer()
                wait(infiniteDefeatInterval)
            end
        end
    end
})
